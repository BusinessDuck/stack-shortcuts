{
  "name": "stack-shortcuts",
  "version": "0.1.2",
  "description": "Easy shortcuts api for working with modern web apps",
  "main": "index.js",
  "scripts": {
    "test": "npm run build | npx mocha -r babel-core/register test/*.spec.js",
    "build": "npm run build:base | npm run build:min",
    "build:base": "babel -d lib/ src/ --source-maps",
    "build:min": "uglifyjs ./lib/shortcuts.js -o ./lib/shortcuts.min.js -c -m --mangle-props --source-map",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BusinessDuck/stack-shortcuts.git"
  },
  "keywords": [
    "npm",
    "shortcuts",
    "hotkeys",
    "boilerplate",
    "javascript",
    "keypress",
    "keydown",
    "keyup",
    "keyboard",
    "key bindings"
  ],
  "author": "Dmitriy Yurov <yurov.de@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BusinessDuck/stack-shortcuts/issues"
  },
  "homepage": "https://github.com/BusinessDuck/stack-shortcuts",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.85.0",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "uglify-js": "^3.4.9"
  }
}
